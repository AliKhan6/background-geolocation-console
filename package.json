{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js --fix && flow",
    "build": "snowpack build && cp build/dist/public/index.html build/index.html",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "jest",
    "server": "better-npm-run start",
    "heroku-postbuild": "rimraf build/ && webpack -p --progress && cross-env NPM_CONFIG_PRODUCTION=true npm prune --production"
  },
  "type": "commonjs",
  "author": "Chris Scott, Transistor Software",
  "license": "MIT",
  "repository": "https://github.com/transistorsoft/background-geolocation-console",
  "betterScripts": {
    "dev": {
      "command": "concurrently --kill-others \"cross-env node --inspect=9229 server/index.js\" \"cross-env snowpack dev\"",
      "env": {
        "TZ": "UTC"
      }
    },
    "start": {
      "command": "node server/index.js",
      "env": {
        "TZ": "UTC",
        "DYNO": "1",
        "NODE_ENV": "production",
        "NPM_CONFIG_PRODUCTION": "true"
      }
    }
  },
